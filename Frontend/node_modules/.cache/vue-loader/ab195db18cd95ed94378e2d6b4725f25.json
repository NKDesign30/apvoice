g } from '@/services/utils';\n\nexport default {\n\n  data() {\n    return {\n      show: false,\n    };\n  },\n\n  head() {\n    return {\n      title: {\n        inner: this.$t('pages.contact.meta.title'),\n      },\n      meta: [\n        descriptionTag(this.$t('pages.contact.meta.description')),\n      ],\n      link: [\n        canonicalTag(this.$route),\n      ],\n    };\n  },\n\n  watch: {\n    $route: {\n      immediate: true,\n      handler(route) {\n        if (route.query.submitted) {\n          this.show = true;\n        } else {\n          this.$router.push('/');\n        }\n      },\n    },\n  },\n};\n</script>\n"]}]}
dline.length > 0;\n    },\n\n    title() {\n      return get(this.page_indicator, 'title', '');\n    },\n\n    content() {\n      return get(this.page_indicator, 'content', []);\n    },\n  },\n\n  methods: {\n    toggleContent() {\n      this.isContentVisible = !this.isContentVisible;\n    },\n  },\n};\n\n</script>\n\n<style lang=\"scss\" scoped>\n\n.page-indicator {\n  @apply -mt-20;\n\n  @screen desktop {\n    @apply -mt-8;\n  }\n\n  &:first-child {\n    @apply mt-0;\n  }\n}\n\n</style>\n"],"sourceRoot":"src/components/content"}]}
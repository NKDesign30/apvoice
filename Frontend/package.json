    "animate.css": "3.7.2",
    "axios": "0.19.0",
    "chart.js": "2.8.0",
    "chartjs-plugin-datalabels": "0.6.0",
    "core-js": "3.3.2",
    "countup.js": "2.0.4",
    "downloadjs": "1.4.7",
    "es6-object-assign": "1.1.0",
    "es6-symbol": "3.1.2",
    "intersection-observer": "0.7.0",

    "babel-eslint": "10.0.3",
    "eslint": "5.16.0",
    "eslint-plugin-vue": "6.0.1",
    "lint-staged": "8.1.5",
    "node-sass": "4.12.0",
    "npm-check-updates": "3.1.20",
    "sass-loader": "8.0.0",
    "svg-sprite-loader": "4.1.6",
    "tailwindcss": "1.0.5",
